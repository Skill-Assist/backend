{
  "name": "skill-assist",
  "version": "0.0.1",
  "description": "Skill Assist: Backend API",
  "author": "Florencio da Silva, M",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start:dev": "export NODE_ENV=dev && nest start --watch",
    "start:debug": "export NODE_ENV=debug && nest start --debug --watch",
    "start:prod": "export NODE_ENV=prod && nest start",
    "test": "export NODE_ENV=test && jest --maxWorkers=1 --verbose",
    "test:watch": "export NODE_ENV=test && jest --watch --maxWorkers=1 --verbose",
    "test:cov": "export NODE_ENV=test && jest --coverage",
    "test:e2e": "export NODE_ENV=test && jest --config ./test/jest-e2e.json --maxWorkers=1 --verbose"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.363.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1409.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "express-session": "^1.17.3",
    "helmet": "^6.1.3",
    "mongodb": "^5.6.0",
    "mongoose": "^7.2.2",
    "mysql2": "^3.4.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typechat": "^0.0.10",
    "typeorm": "^0.3.12",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@types/unzipper": "^0.10.6",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
